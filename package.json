{
  "name": "karting-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run lint && npm run format",
    "build": "next build && next-image-export-optimizer",
    "start": "serve ./out",
    "start:ssr": "next start",
    "docker:dev": "docker compose -f docker-compose.dev.yaml up --build --watch",
    "docker:prod": "docker compose up -d --build",
    "lint": "next build && eslint .",
    "format": "prettier --write src/app/"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "serve": "^14.2.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "next-image-export-optimizer": "^1.19.0"
  }
}
